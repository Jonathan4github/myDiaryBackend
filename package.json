{
  "name": "mydiarybackend",
  "version": "1.0.0",
  "description": "an online journal where users can pen down their thoughts and feelings",
  "main": "app.js",
  "scripts": {
    "mocha": "mocha --compilers js:babel-register",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 500000 --exit --compilers js:babel-register --recursive ./server/test/",
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec npm run babel-node -- server/app.js",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jonathan4github/myDiaryBackend.git"
  },
  "author": "Jonathan Williams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jonathan4github/myDiaryBackend/issues"
  },
  "homepage": "https://github.com/Jonathan4github/myDiaryBackend#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3"
  }
}
